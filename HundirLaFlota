#include <iostream>
using namespace std;

int main() {
    int Tableroixj = 10;
    char matriz[10][10];

    for (int i = 0; i < Tableroixj; i++) {
        for (int j = 0; j < Tableroixj; j++) {
            matriz[i][j] = '~' ;
        }
    }

    for (int i = 0; i < Tableroixj; i++) {

        for (int j = 0; j < Tableroixj; j++) {
            cout << matriz[i][j] << " ";
        }

        cout << endl;
    }
    
}
