#include <iostream>
using namespace std;

int main() {
   const int Tableroixj = 10; 
    char matriz[Tableroixj][Tableroixj];

    for (int i = 0; i < Tableroixj; i++) {
        for (int j = 0; j < Tableroixj; j++) {
            matriz[i][j] = '~' ; 
        }
    }

    for (int i = 0; i < Tableroixj; i++) { 

        for (int j = 0; j < Tableroixj; j++) { 
            cout << matriz[i][j] << " "; 
        }

        cout << endl; 
    }
    
}
